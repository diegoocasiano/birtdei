{
    "version": 2,
    "builds": [
      {
        "src": "/birtdei.py",
        "use": "@vercel/python"
      }
    ],  
    "routes": [
      {
        "src": "/react-components/dist/(.*)",
        "dest": "/react-components/dist/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/birtdei.py"
      }
    ]
  }